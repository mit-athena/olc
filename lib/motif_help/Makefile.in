SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
datadir=@datadir@

MOTIF_HELP_DIR=${libdir}/olc/motif_help
MOTIF_HELP_FILES=olc.ask olc.init.contq olc.init.newq olc.replay

default: all

all clean distclean depend.in:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${MOTIF_HELP_DIR}
	foreach i in $(MOTIF_HELP_FILES); do \
	    ${INSTALL} -m 444 ${srcdir}/$$i ${DESTDIR}${MOTIF_HELP_DIR}; done
