SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
mandir=@mandir@

MAN1PAGES=olc olcr olc_answers cref olist oreplay oshow

all clean distclean depend.in:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${mandir}/man1
	for i in $(MAN1PAGES); do \
	    ${INSTALL} -m 444 ${srcdir}/$$i.1 ${DESTDIR}${mandir}/man1; done
