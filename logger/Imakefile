L_SRCS=	log.c
L_OBJS=	log.o

D_SRCS= bbd.c
D_OBJS= bbd.o

NormalLibraryTarget(logger,$(L_OBJS))
SaberLibraryTarget(logger,$(L_SRCS),$(L_OBJS))

NormalProgramTarget(bbd,$(D_OBJS),,,)
SaberProgramTarget(bbd,$(D_SRCS),$(D_OBJS),,)

DependSrcs()

LintTarget()

AllTarget(liblogger.a bbd)
