SRCS = writed.c
OBJS = writed.o

NormalProgramTarget(writed,$(OBJS),,,)
SaberProgramTarget(writed,$(SRCS),$(OBJS),,)

DependSrcs()

LintTarget()

#ifdef InstallAltLoc
InstallProgram(writed,$(DESTDIR)/etc/athena)
#else
InstallProgram(writed,$(DESTDIR)/etc)
#endif

AllTarget(writed)
