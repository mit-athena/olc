 * $Source: /afs/dev.mit.edu/source/repository/athena/bin/olc/server/rpd/Imakefile,v $
 * $Id: Imakefile,v 1.11 1991-03-24 18:31:37 lwvanels Exp $
 */

/* #define Profiling */

#ifdef Profiling
LIBSUFFIX=_p
EXTRA_LOAD_FLAGS=-pg
#endif

SRCS=	rpd.c fdcache.c handle_request.c io.c acl_files.c kopt.c get_nm.c \
	strcasecmp.c

OBJS=	rpd.o fdcache.o handle_request.o io.o acl_files.o kopt.o get_nm.o \
	strcasecmp.o

#ifdef Profiling
DEFINES= -DPROFILE
.c.o:
	$(CC) -pg -c $(CFLAGS) $*.c
#else
DEFINES=
#endif /* Profiling */


SYSTEM_LIBRARIES=	$(KerberosLib)

INCLUDES= -I.

NormalProgramTarget(rpd,$(OBJS),,$(SYSTEM_LIBRARIES),)

SaberProgramTarget(rpd,$(SRCS),$(OBJS),$(SYSTEM_LIBRARIES),)

depend: Makefile
	$(DEPEND) -s "# DO NOT DELETE" -- $(ALLDEFINES) *.[c] *.[h]

LintTarget()

#ifdef InstallAltLoc
InstallProgram(rpd,/mit/olcdev/@sys)
#else
InstallProgram(rpd,$(DESTDIR)/usr/etc)
#endif

AllTarget(rpd)
