SHELL=/bin/sh
VPATH=@srcdir@

# "osend" directory isn't listed because it doesn't produce a useful binary.
# [I'd say "yet", but I don't think it's a matter of time. =)]  --bert

SUBDIRS=lib parser tty olc @SUBDIR_MOTIF@ oreplay

default: all

all clean distclean install depend.in:
	for i in ${SUBDIRS}; do (cd $$i; ${MAKE} ${MFLAGS} $@) || exit 1; done
